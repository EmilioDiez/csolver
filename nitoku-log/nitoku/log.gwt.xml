<?xml version="1.0" encoding="UTF-8"?>
<module>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.core.Core' />

  <inherits name="com.google.gwt.logging.Logging"/>
  <!--  Logger level: The levels are in descending order.
    	severe (highest value)
 		finest  (lowest value) -->
  <define-property name="logLevel" values="severe,warning,info,config,fine,finer,finest" />
  <set-property name="logLevel" value="info" />
  <set-property name="jre.debugMode" value="ENABLED" />
  
  <source path='log'/>
  
</module>
